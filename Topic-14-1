#include <stdio.h>
#include <locale.h>
#include <stdlib.h>
#include <math.h>

int main ()
{
    char *locale = setlocale(LC_ALL, "");
    int b;
    int a;
    float answer = 0;
    printf("Введите B: ");
    scanf("%d", &b);
    printf("Введите A: ");
    scanf("%d", &a);
    for (int i = a; i<=b; i+=1)
    {
            for (int j = 1; j<=i; j+=1)
            {
            printf("%d \n", i);
            }
    }
    return 0;
}
